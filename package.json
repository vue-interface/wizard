{
  "name": "@vue-interface/wizard",
  "version": "0.9.2",
  "description": "A Vue wizard component.",
  "main": "index.js",
  "scripts": {
    "watch": "npm run serve",
    "serve": "vue-cli-service build --target lib --name=Wizard ./index.js --watch",
    "build": "vue-cli-service build --target lib --name=Wizard ./index.js",
    "lint": "vue-cli-service lint",
    "fix": "vue-cli-service lint --fix",
    "pre-release": "npm run build; git add . -A; git commit -m 'pre-release commit'",
    "release-patch": "npm run pre-release && npm version patch -m \"%s\" && npm run release;",
    "release-minor": "npm run pre-release && npm version minor -m \"%s\" && npm run release;",
    "release-major": "npm run pre-release && npm version major -m \"%s\" && npm run release;",
    "release": "git add . -A; git commit; git push --tags origin; npm publish;",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:dev": "./node_modules/.bin/vuepress dev docs",
    "docs:build": "./node_modules/.bin/vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-interface/wizard.git"
  },
  "keywords": [
    "Form",
    "Control",
    "Vue",
    "Component",
    "Tailwind",
    "Bootstrap"
  ],
  "author": "Justin Kimbrell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vue-interface/wizard/issues"
  },
  "homepage": "https://github.com/vue-interface/wizard/docs#readme",
  "dependencies": {
    "@vue-interface/btn": "^0.9.5",
    "@vue-interface/btn-activity": "^0.9.5",
    "@vue-interface/btn-group": "^0.9.0",
    "@vue-interface/sizeable": "^0.9.1",
    "@vue-interface/slide-deck": "^0.9.5",
    "@vue-interface/utils": "^0.9.13",
    "@vue-interface/variant": "^1.0.11",
    "color": "^3.1.2",
    "core-js": "^3.6.4",
    "hex-to-rgba": "^2.0.1",
    "postcss-nested": "^4.2.3",
    "tailwindcss": "^1.6.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0",
    "@vue/cli-plugin-eslint": "^4.0",
    "@vue/cli-service": "^4.0",
    "@vuepress/plugin-register-components": "^1.5.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "markdown-it-meta": "0.0.1",
    "postcss-cli": "^7.1.1",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.3",
    "vuepress-plugin-template-constants": "^0.2.1",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
